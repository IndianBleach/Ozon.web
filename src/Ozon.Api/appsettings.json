{
  "ConnectionStrings": {
    "MssqlConnectionString": "Server=serv-products-mssql,1433;Initial Catalog=Ozon.Products;User ID=sa;Password=Sample123!;Encrypt=False"
  },
  "ReverseProxy": {
    "Routes": {
      "catalog-route": {
        "ClusterId": "catalogs-cluster",
        "Match": {
          "Path": "catalogs/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catchall}"
          }
        ]
      },
      "storages-route": {
        "ClusterId": "storages-cluster",
        "Match": {
          "Path": "storages/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "storages/{**catchall}"
          }
        ]
      },
      "accounts-route": {
        "ClusterId": "accounts-cluster",
        "Match": {
          "Path": "accounts/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catchall}"
          }
        ]
      },
      "authorization-route": {
        "ClusterId": "authorization-cluster",
        "Match": {
          "Path": "authorize/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catchall}"
          }
        ]
      },
      "products-route": {
        "ClusterId": "products-cluster",
        "Match": {
          "Path": "products/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catchall}"
          }
        ]
      }
    },
    "Clusters": {
      "products-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7162"
          }
        }
      },
      "catalogs-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://srv-catalogs"
          }
        }
      },
      "storages-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://srv-storages"
          }
        }
      },
      "authorization-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://authorize-api"
          }
        }
      },
      "accounts-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://accounts-api"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
